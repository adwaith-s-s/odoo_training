<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="action_report_product_warranty" model="ir.actions.report">
        <field name="name">Product Warranty</field>
        <field name="model">product.warranty.report.wizard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">product_warranty.report_warranty_detail</field>
